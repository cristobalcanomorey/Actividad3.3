<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE configuration PUBLIC '-//mybatis.org//DTD Config 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-config.dtd'>
<configuration>
	<properties resource='jdbc.properties' />
	<typeAliases>
		<typeAlias type='aplicacion.modelo.pojo.AltaBaja' alias='AltaBaja'></typeAlias>
		<typeAlias type='aplicacion.modelo.pojo.Calculo' alias='Calculo'></typeAlias>
		<typeAlias type='aplicacion.modelo.pojo.Mail' alias='Mail'></typeAlias>
		<typeAlias type='aplicacion.modelo.pojo.Usuario' alias='Usuario'></typeAlias>
	</typeAliases>
	<environments default='development'>
		<environment id='development'>
			<transactionManager type='JDBC' />
			<dataSource type='POOLED'>
				<property name='driver' value='${jdbc.driverClassName}' />
				<property name='url' value='${jdbc.url}' />
				<property name='username' value='${jdbc.username}' />
				<property name='password' value='${jdbc.password}' />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource='aplicacion/modelo/dao/mappers/AltaBajaMapper.xml' />
		<mapper resource='aplicacion/modelo/dao/mappers/CalculoMapper.xml' />
		<mapper resource='aplicacion/modelo/dao/mappers/MailMapper.xml' />
		<mapper resource='aplicacion/modelo/dao/mappers/UsuarioMapper.xml' />
	</mappers>
</configuration>